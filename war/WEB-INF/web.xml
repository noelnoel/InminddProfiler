<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
    <servlet-name>Inmindd</servlet-name>
    <servlet-class>com.inmindd.dcu.server.InminddServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>Inmindd</servlet-name>
    <url-pattern>/inminddprofiler/Inmindd</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>InminddScore</servlet-name>
    <servlet-class>com.inmindd.dcu.server.InminddServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>InminddScore</servlet-name>
    <url-pattern>/Score/Inmindd</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>InminddGoals</servlet-name>
    <servlet-class>com.inmindd.dcu.server.InminddServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>InminddGoals</servlet-name>
    <url-pattern>/Goals/Inmindd</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>InminddRiskFactorInfos</servlet-name>
    <servlet-class>com.inmindd.dcu.server.InminddServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>InminddRiskFactorInfos</servlet-name>
    <url-pattern>/RiskFactorInfos/Inmindd</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>InminddLandingPage</servlet-name>
    <servlet-class>com.inmindd.dcu.server.InminddServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>InminddLandingPage</servlet-name>
    <url-pattern>/LandingPage/Inmindd</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
